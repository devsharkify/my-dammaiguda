      "src": "/screenshots/dashboard-wide.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Dashboard on Desktop"
    }
  ],
  "shortcuts": [
    {
      "name": "Report Issue",
      "short_name": "Report",
      "description": "Report a civic issue in your area",
      "url": "/report",
      "icons": [{ "src": "/icons/icon-192x192.png", "sizes": "192x192" }]
    },
    {
      "name": "Kaizer Fit",
      "short_name": "Fitness",
      "description": "Track your health and fitness",
      "url": "/fitness",
      "icons": [{ "src": "/icons/icon-192x192.png", "sizes": "192x192" }]
    },
    {
      "name": "News",
      "short_name": "News",
      "description": "Read latest local news",
      "url": "/news",
      "icons": [{ "src": "/icons/icon-192x192.png", "sizes": "192x192" }]
    },
    {
      "name": "Astrology",
      "short_name": "Astrology",
      "description": "Check your horoscope",
      "url": "/astrology",
      "icons": [{ "src": "/icons/icon-192x192.png", "sizes": "192x192" }]
    }
  ],
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.mydammaiguda.app",
      "id": "com.mydammaiguda.app"
    }
  ],
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "media",
          "accept": ["image/*", "video/*"]
        }
      ]
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+dammaiguda",
      "url": "/?action=%s"
    },
    {
      "protocol": "web+report",
      "url": "/report?data=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/report",
      "accept": {
        "image/*": [".jpg", ".jpeg", ".png", ".gif", ".webp"]
      }
    }
  ],
  "scope_extensions": [
    {
      "origin": "https://*.emergentagent.com"
    }
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "widgets": [
    {
      "name": "AQI Widget",
      "description": "Shows current air quality index",
      "tag": "aqi",
      "template": "aqi-widget",
      "ms_ac_template": "aqi-widget-ac",
      "data": "/api/aqi/current",
      "type": "application/json",
      "screenshots": [
        {
          "src": "/screenshots/widget-aqi.png",
          "sizes": "300x200",
          "label": "AQI Widget Preview"
        }
      ],
      "icons": [
        {
          "src": "/icons/icon-192x192.png",
          "sizes": "192x192"
        }
      ],
      "backgrounds": [
        {
          "src": "/icons/icon-512x512.png",
          "sizes": "512x512"
        }
      ]
    }
  ],
  "note_taking": {
    "new_note_url": "/report"
  }
}
